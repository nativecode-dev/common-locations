{
  "debug": {
    "js": {
      "title": "js"
    },
    "json": {
      "title": "json"
    }
  },
  "destination": {
    "lib": "lib"
  },
  "env": {
    "common": {
      "vars": {}
    },
    "debug": {
      "vars": {
        "NODE_ENV": "debug"
      }
    },
    "production": {
      "vars": {
        "NODE_ENV": "production"
      }
    }
  },
  "plugins": {
    "babel": {
      "presets": "es2015"
    },
    "mocha": {
      "reporter": "spec"
    }
  },
  "sources": {
    "js": ["src/**/*.js"],
    "json": ["src/**/*.json"],
    "tests": ["tests/**/*.js"]
  }
}
